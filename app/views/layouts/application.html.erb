<!DOCTYPE html>
<html>
  <head>
    <title>PrivateEvents</title>
    <%= render 'layouts/rails_default' %>
    <%= render 'layouts/shim' %>
    <%= javascript_include_tag "application" %>
  </head>

  <body>
  <%= render 'layouts/header' %>
  <% flash.each do |message_type, message| %>
    <%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
  <% end %>
    <%= yield %>
  </body>
</html>
